#!/usr/bin/env node

var commander = require('commander');

commander
  .description('Manage datasets.')
  .arguments('<subcommand>')
  .command('list', 'List datasets.')
  .command('push', 'Push a new dataset.')
  .command('pull [key]', 'Pull an existing dataset.')
  .command('rm [key]', 'Remove a dataset.')
  .parse(process.argv);
