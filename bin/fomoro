#!/usr/bin/env node

var program = require('commander');

program
  .version('0.0.1')
  .description('Fomoro toolbelt')
  .arguments('<command> <config>')
  .command('upload <config>', 'upload a model or dataset')
  .command('download <config>', 'upload a model or dataset')
  .command('train <config>', 'train a model')
  .action(function() {})
  .parse(process.argv);
