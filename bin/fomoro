#!/usr/bin/env node

var commander = require('commander');

commander
  .version('0.0.1')
  .description('Fomoro toolbelt.')
  .arguments('<subcommand>')

  .command('login', 'Login to Fomoro.')

  .command('model <subcommand>', 'Manage models.')
  .command('data <subcommand>', 'Manage datasets.')
  .command('session <subcommand>', 'Manage sessions.')
  .command('checkpoint <subcommand>', 'Manage checkpoints.')

  .parse(process.argv);
