#!/usr/bin/env node

var commander = require('commander');

commander
  .description('Manage models.')
  .arguments('<subcommand>')
  .command('list', 'List models.')
  .command('create', 'Create a new model.')
  .command('rm [model]', 'Remove a model.')
  .command('pull [sha]', 'Pull an existing model.')
  .command('rm-artifact [sha]', 'Remove a model artifact.')
  .parse(process.argv);
