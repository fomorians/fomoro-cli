#!/usr/bin/env node

var commander = require('commander');

commander
  .description('Manage sessions.')
  .arguments('<subcommand>')
  .command('start', 'Start a training session.')
  .command('stop', 'Stop a training session.')
  .command('info', 'Inspect a training session.')
  .command('watch', 'Watch a training session.')
  .command('list', 'List all training sessions.')
  .parse(process.argv);
